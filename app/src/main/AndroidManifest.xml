<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kalusyu.bigfrontend">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="com.kalusyu.bigfrontend.EndApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.kalusyu.bigfrontend.h264decode.H264Activity"></activity>
        <activity android:name="com.kalusyu.bigfrontend.custominput.KeyboardActivity"></activity>
        <activity android:name="com.kalusyu.bigfrontend.DemoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kalusyu.bigfrontend.MainActivity" />
        <activity
            android:name="com.kalusyu.bigfrontend.mediacodec.MediaCodecActivity"
            android:label="MediaCodecTest" />
        <activity android:name="com.kalusyu.bigfrontend.camerax.CameraActivity" />
        <activity android:name="com.kalusyu.bigfrontend.YuvActivity" />
    </application>

</manifest>